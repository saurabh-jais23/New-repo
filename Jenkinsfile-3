pipeline {
  agent { label 'ubuntu' }

  stages {
    stage('Hello') {
      steps {
        sh 'pip install pywinrm'
        sh 'pip install request'
        sh 'python -c "import winrm; print(/'WinRM module is installed/')"'
'
        sh 'ansible-playbook -i winhosts2.ini copy_to_win.yml'
      }
    }
  }
}
